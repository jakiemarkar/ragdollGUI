	function spamed()
	wait(0.3)
	local A_1 = "Go Sub To jakiemarkar On Youtube" local A_2 = "All" 
	local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2)
	spamed()
	end
	spamed()
	end)
end
